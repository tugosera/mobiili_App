<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
         xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
         x:Class="mobiili_App.NewPage5"
         Title="NewPage5">
    <StackLayout Padding="20">
        <Entry x:Name="phoneEntry" Placeholder="Enter phone number" />
        <Entry x:Name="email_phone" Placeholder="Enter email address" />
        <Button Text="Send SMS" Clicked="Saada_sms_Clicked"/>
        <Button Text="Send Email" Clicked="Saada_email_Clicked"/>
    </StackLayout>
</ContentPage>